{
    "name": "windwalker/io",
    "type": "windwalker-package",
    "description": "Windwalker IO package",
    "keywords": ["windwalker", "framework", "io"],
    "homepage": "https://github.com/ventoviro/windwalker-io",
    "license": "LGPL-2.0-or-later",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*"
    },
    "require-dev": {
        "windwalker/test": "~3.0",
        "windwalker/filter": "~3.0"
